{
  "name": "friksi",
  "version": "0.1.0",
  "private": true,
  "description": "A next-generation civic engagement platform with democratic moderation and anti-bot protection",
  "author": "Friksi Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.1",
  "scripts": {
    "build": "next build",
    "dev": "next dev --port 3000",
    "lint": "next lint",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "db:types": "supabase gen types typescript --local > src/lib/database/types/database.types.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push",
    "db:seed": "supabase seed",
    "db:seed:additional": "psql postgresql://postgres:postgres@localhost:54322/postgres -f supabase/seed-additional.sql",
    "colima:setup": "ln -sf $HOME/.colima/default/docker.sock /tmp/docker.sock && echo 'Colima Docker socket symlink created at /tmp/docker.sock'",
    "supabase:start": "export DOCKER_HOST='unix:///tmp/docker.sock' && supabase start --exclude vector",
    "supabase:stop": "export DOCKER_HOST='unix:///tmp/docker.sock' && supabase stop",
    "supabase:status": "export DOCKER_HOST='unix:///tmp/docker.sock' && supabase status"
  },
  "dependencies": {
    "@mantine/carousel": "^8.3.2",
    "@mantine/core": "^8.3.2",
    "@mantine/form": "^8.3.2",
    "@mantine/hooks": "^8.3.2",
    "@mantine/modals": "^8.3.2",
    "@mantine/notifications": "^8.3.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.7",
    "@tabler/icons-react": "^3.35.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "embla-carousel": "^8.5.2",
    "embla-carousel-react": "^8.5.2",
    "lucide-react": "^0.344.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "supabase": "^1.142.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
